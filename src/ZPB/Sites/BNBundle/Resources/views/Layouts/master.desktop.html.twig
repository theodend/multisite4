{% set active_site = (active_site is defined) ? active_site : null %}
{% set active_page = (active_page is defined) ? active_page : null %}
<!doctype html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{% if block('title') %}{{ block('title') }} {% else %}Bienvenue {% endif %}| ZooParc de Beauval</title>
    {% if block('description') %}
        <meta name="description" content="{{ block('description') }}"/>
    {% endif %}
    {% if block('keywords') %}
        <meta name="keywords" content="{{ block('keywords') }}"/>
    {% endif %}

    {% block metas_fb %}{% endblock %}

    {% block headLinks %}
        <link rel="stylesheet" href="/css/sites/bn/master.css"/>
        <link rel="stylesheet" href="/css/font-awesome.min.css"/>
    {% endblock %}

    {% block headStyles %}{% endblock %}

    {% block headScripts %}
        <script src="/js/vendor/lib.js"></script>
        <script src="/js/sites/bn/main.js"></script>
    {% endblock %}
</head>
